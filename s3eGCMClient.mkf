	android-external-jars="lib/android/libgoogleplayservice/android-support-v4.jar"
	android-external-jars="lib/android/libgoogleplayservice/google-play-services.jar"
    android-external-jars="lib/android/s3eGCMClient.jar"
    android-so="lib/android/libs3eGCMClient.so"
}
